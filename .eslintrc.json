{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:import/recommended",
      "plugin:import/typescript",
      "plugin:react-hooks"
    ],
    "parser": "@typescript-eslint/parser",
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [["src", "./src"]],
          "extensions": [".ts", ".tsx"]
        }
      }
    },
    "rules": {}
  }